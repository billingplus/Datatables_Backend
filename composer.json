    "license": "GPL-3.0-or-later",
    "keywords": [
      "laravel",
      "database",
      "sql",
      "orm",
      "datatables"
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/billingplus/Datatables_Backend"
      }
    ],
